= semantic_form_for [:admin, @venue] do |f|
  = f.inputs do
    = f.input :location
    = f.input :url
    = f.input :street
    = f.input :zipcode
    = f.input :city
    = f.input :region, :as => :select
    = f.input :country, collection: {"Deutschland" => "DE", "Niederlande" => "NL"}, as: :select

  = f.button
